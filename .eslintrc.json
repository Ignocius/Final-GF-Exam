{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
      "var1": true,
      "var2": false
    },
    "rules": {
        "semi": 2,
        "arrow-body-style": ["error", "always"],
        "quotes": [2, "double", "avoid-escape"]
    }
}
